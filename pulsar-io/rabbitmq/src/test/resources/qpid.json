{
  "name": "EmbeddedBroker",
  "modelVersion": "2.0",
  "storeVersion": 1,
  "authenticationproviders": [
    {
      "name": "noPassword",
      "type": "Anonymous",
      "secureOnlyMechanisms": []
    },
    {
      "name": "PasswordPath",
      "type": "PlainPasswordPath",
      "path": "${qpid.home_dir}${file.separator}etc${file.separator}passwd",
      "secureOnlyMechanisms": []
    }
  ],
  "ports": [
    {
      "name": "AMQP",
      "port": "${qpid.amqp_port}",
      "authenticationProvider": "PasswordPath",
      "protocols": [
        "AMQP_0_9_1"
      ]
    }
  ],
  "virtualhostnodes": [
    {
      "name": "default",
      "type": "JSON",
      "defaultVirtualHostNode": "true",
      "virtualHostInitialConfiguration": "${qpid.initial_config_virtualhost_config}",
      "storeType": "DERBY"
    }
  ]
}
